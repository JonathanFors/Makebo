<div style="background-color: {{section.settings.background-color}}" class="background-color"><div class="padding-global"><div class="container-medium"><div class="padding-section-medium"><div class="feature-list_component"><div class="w-layout-grid feature-list_content is-secondary"><div class="feature-list_content-left"><div class="margin-bottom margin-medium"><h2>
{{ section.settings.text_primary_heading }}
</h2>
</div>{% if section.settings.checkbox_toggle_buttons == true %}
<div class="margin-bottom margin-medium"><div class="button-group is-centered"><a data-wf--module-button-primary--variant="base" href="{{ section.settings.url_primary_button_link }}" class="button is-icon w-inline-block"> <div>{{ section.settings.text_primary_button_label }}</div><div data-w-id="64fe5f88-e72c-dffb-a813-38578064b7ca" class="icon-embed-xsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 20 8" fill="none" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><path d="M1 4L16 4L15.261 4" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path><path d="M16 6.39998V1.59998L19.2 3.99998L16 6.39998Z" fill="currentColor" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path></svg></div> </a>
<a href="{{ section.settings.url_secondary_button_link }}" class="button is-alternate w-inline-block"> <div>{{ section.settings.text_secondary_button_label }}</div> </a>
</div></div>{% endif %}
</div><div class="feature-list_content-right"><div li-content-for-blocks="" class="w-layout-grid feature-list_item-list is-alternate">
{% for block in section.blocks %}
    {% case block.type %}
        {% when '@app' %}
            {% render block %}
    {% when 'feature-item' %}
            <div li-block="Feature item" class="feature-list_item is-max-width">
                    
                    <div class="margin-bottom margin-xsmall"><h3 class="heading-style-h6">
{{ block.settings.text_card_heading_1 }}
</h3>
</div>
                    
                    <p> {{ block.settings.textarea_card_description_1 }} </p>

                    
                </div>
        {% endcase %}
{% endfor %}

</div>

</div></div></div></div></div></div></div><div class="hide w-embed"></div>
