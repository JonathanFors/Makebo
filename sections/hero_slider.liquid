"class":"section_hero-swiper","blocks":[{"type":"@app"},{"name":"Slide","type":"slide","settings":[{"id":"image_background_image","type":"image_picker","label":"Background Image"},{"id":"text_heading","type":"text","label":"Heading","default":"SHORT HEADING GOES HERE"},{"id":"textarea_paragraph","type":"textarea","label":"Paragraph","default":"Lorem ipsum dolor sit amet, consectetur adipiscing elit. Suspendisse varius enim in eros elementum tristique. Duis cursus, mi quis viverra ornare, eros dolor interdum nulla, ut commodo diam libero vitae erat. Aenean faucibus nibh et justo cursus id rutrum lorem imperdiet. Nunc ut sem vitae risus tristique posuere."},{"id":"url_primary_button_link","type":"url","label":"Primary Button -> Link"},{"id":"text_primary_button_label","type":"text","label":"Primary Button -> Label","default":"Shop now"},{"id":"url_secondary_button_link","type":"url","label":"Secondary Button -> Link"},{"id":"text_secondary_button_label","type":"text","label":"Secondary Button -> Label","default":"view collections"},{"type":"color","id":"overlay-color","label":"Overlay Color","default":"#ffffff"}]}],"presets":[{"name":"hero-slider","category":"Liquify"}]} {% endschema %}