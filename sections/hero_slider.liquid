":"url_primary_button_link","type":"url","label":"Primary Button -> Link"},{"id":"text_primary_button_label","type":"text","label":"Primary Button -> Label","default":"Shop now"},{"id":"url_secondary_button_link","type":"url","label":"Secondary Button -> Link"},{"id":"text_secondary_button_label","type":"text","label":"Secondary Button -> Label","default":"view collections"},{"type":"color","id":"overlay-color","label":"Overlay Color","default":"#ffffff"}]}],"presets":[{"name":"hero-slider","category":"Liquify","blocks":[{"type":"slide"}]}]} {% endschema %}