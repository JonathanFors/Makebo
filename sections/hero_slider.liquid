<div class="hero-swiper_component"><div swiper="instance" class="swiper"><div li-content-for-blocks="" class="swiper_wrapper">
{% for block in section.blocks %}
    {% case block.type %}
        {% when '@app' %}
            {% render block %}
    {% when 'slide' %}
            <div li-block="Slide" class="swiper_slide is-home-hero">
                    <div class="swiper-slide_inner-wrapper"><img loading="eager" alt="{{ block.settings.image_background_image.alt }}" src="{{ block.settings.image_background_image | img_url: 'master' }}" class="slider-image"}>
<div class="hero-swiper_content"><div class="padding-global is-large"><div class="container-large"><div class="max-width-large"><div class="margin-bottom margin-medium"><h1>
{{ block.settings.text_heading }}
</h1>
</div><div class="margin-bottom margin-large"><p> {{ block.settings.textarea_paragraph }} </p>
</div><div class="button-group"><a data-wf--module-button-primary--variant="base" href="{{ block.settings.url_primary_button_link }}" class="button is-icon w-inline-block"> <div>{{ block.settings.text_primary_button_label }}</div><div data-w-id="64fe5f88-e72c-dffb-a813-38578064b7ca" class="icon-embed-xsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 20 8" fill="none" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><path d="M1 4L16 4L15.261 4" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path><path d="M16 6.39998V1.59998L19.2 3.99998L16 6.39998Z" fill="currentColor" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path></svg></div> </a>
<a href="{{ block.settings.url_secondary_button_link }}" class="button is-alternate w-inline-block"> <div>{{ block.settings.text_secondary_button_label }}</div> </a>
</div></div></div></div></div><div style="background-color: {{block.settings.overlay-color}}" id="" class="slider-image_overlay"></div></div>
                    <div class="hide w-embed"></div>

                </div>
        {% endcase %}
{% endfor %}

</div>

