<div style="background-color: {{section.settings.background-color}}" class="background-color is-gray3"><div data-url="{{ routes.product_recommendations_url }}?section_id={{ section.id }}&product_id={{ product.id }}&limit=4&intent=related" class="padding-global padding-section-medium product-recommendations"><div class="container-large">{% if section.settings.checkbox_display_title_paragraph == true %}
<div class="title-wrapper"><div class="max-width-large align-center"><div class="text-align-center"><div class="margin-bottom margin-small"><h2>
{{ section.settings.text_heading }}
</h2>
</div><div class="margin-bottom margin-medium"><p>
{{ section.settings.text_paragraph }}
</p>
</div></div></div></div>{% endif %}
<div class="product_list-wrapper"><ul role="list" class="product_list w-list-unstyled">{% for product in recommendations.products limit: 4 %}    <li li-for="product in recommendations.products limit: 4" class="product_list-item">
                                    
                            <div class="products_item"><script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<div li-element="product-variant-container" class="products_variant-wrapper is-vertical" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" x-init="$nextTick(() => { activateVariants() })" @setbundleitem.window="updateBundleItem($event.detail.product, $event.detail.quantity)"><div class="products_image-wrapper"><img li-object:src="product.featured_image | image_url: width: 500" li-object:alt="product.featured_image.alt" alt="{{ product.featured_image.alt }}" src="{{ product.featured_image | image_url: width: 500 }}" loading="lazy" class="products_image is-vertical">
<img li-object:src="product.images[1] | img_url: width: 500" li-object:alt="product.images[1].alt" alt="{{ product.images[1].alt }}" src="{{ product.images[1] | img_url: width: 500 }}" loading="lazy" class="products_image is-vertical is-secondary">
<div class="products_image-overlay"><div class="products_overlay-content"><div class="button-group is-horizontal">{% if product.available == true %}    <div li-if="product.available == true" class="max-width-full">
    <script src="{{ 'liquify_custom.js' | asset_url }}" type="text/javascript"></script>
<a li-element="direct-add-to-cart" href="#" class="button is-product w-inline-block" x-data="handleVariant({{ product | json | escape }}, {{ product.options_by_name | json | escape }}, {{ product.selected_or_first_available_variant | json | escape }})" @click="addToCart(event, true)"><div>Add to cart</div><div class="icon-embed-xxsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 32 32" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><title>bag-check</title><g fill="currentColor" stroke-linecap="round" stroke-linejoin="round"><path d="M11 13V6C11 3.239 13.239 1 16 1C18.761 1 21 3.239 21 6V13" stroke="currentColor" stroke-width="2" fill="none"></path><path d="M14.9999 29H6.77109C4.84184 29 3.41411 27.2053 3.84792 25.3254L5.99994 16V9H25.9999V16L26.4615 18" stroke="currentColor" stroke-width="2" fill="none"></path><path d="M18 25L22 29L30 21" stroke="currentColor" stroke-width="2" fill="none"></path></g></svg></div></a>    </div>
{% endif %}{% if product.available != true %}    <div li-if="product.available != true" class="button is-disabled">
    <div>Sold out</div><div class="icon-embed-xxsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 24 24" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><title>bag-xmark</title><g fill="currentColor" stroke-linecap="square" stroke-linejoin="miter" stroke-miterlimit="10"><path d="M9 9V4C9 2.34315 10.3431 1 12 1V1C13.6569 1 15 2.34315 15 4V9" stroke="currentColor" stroke-width="2" fill="none"></path><path d="M19 11V6H12H5.00001V12L3.47843 19.6078C3.2309 20.8453 4.17749 22 5.43959 22H10.5" stroke="currentColor" stroke-width="2" fill="none"></path><path d="M15 22L22 15" stroke="currentColor" stroke-width="2" fill="none"></path><path d="M15 15L22 22" stroke="currentColor" stroke-width="2" fill="none"></path></g></svg></div>    </div>
{% endif %}</div></div></div></div><a li-object:href="product.url" href="{{ product.url }}" class="product_content is-vertical w-inline-block"><div class="margin-bottom margin-xsmall"><div class="margin-bottom margin-small"><div class="products_title-wrapper"><div class="product6_left"><div li-object="product.title" class="product-title">{{ product.title }}</div><div class="product_meta-wrapper"><div li-object="product.type" class="text-style-capitalize text-size-small text-style-muted">{{ product.type }}</div><div class="text-style-capitalize text-size-small text-style-muted">â€“</div><div li-object="product.selected_or_first_available_variant.title" class="text-style-capitalize text-size-small text-style-muted">{{ product.selected_or_first_available_variant.title }}</div></div></div><div class="flex-h is-gap-tiny"><div li-object="product.price | money" class="product-price">{{ product.price | money }}</div>{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price" class="li-if">
    <div li-object="product.compare_at_price | money" class="products_compare-at-price">{{ product.compare_at_price | money }}</div>    </div>
{% endif %}</div></div></div></div><div class="flex-h is-space-between"><div class="button is-tiny is-link is-icon"><div>View details</div><div class="icon-embed-xxsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 20 8" fill="none" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><path d="M1 4L16 4L15.261 4" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path><path d="M16 6.39998V1.59998L19.2 3.99998L16 6.39998Z" fill="currentColor" stroke="currentColor" stroke-width="1.6" stroke-linecap="round" stroke-linejoin="round"></path></svg></div></div></div></a><div class="products_label-list">{% if product.compare_at_price_max > product.price %}    <div li-if="product.compare_at_price_max > product.price" class="products_label">
    <div class="products_label-icon w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 24 24"><title>discount-2</title><g fill="#010503" stroke-linecap="round" stroke-linejoin="round"><polygon fill="none" stroke="currentcolor" stroke-width="2" points="19,15 22,12 19,9 19,5 15,5 12,2 9,5 5,5 5,9 2,12 5,15 5,19 9,19 12,22 15,19 19,19 "></polygon><line fill="none" stroke="currentcolor" stroke-width="2" x1="15" y1="9" x2="9" y2="15"></line><circle data-stroke="none" fill="currentcolor" cx="9.5" cy="9.5" r="1.5" stroke-linejoin="miter" stroke="none"></circle><circle data-stroke="none" fill="currentcolor" cx="14.5" cy="14.5" r="1.5" stroke-linejoin="miter" stroke="none"></circle></g></svg></div><div>Sale</div>    </div>
{% endif %}{% if product.selected_or_first_available_variant.inventory_quantity > 0 %}    <div li-if="product.selected_or_first_available_variant.inventory_quantity > 0" class="li-if">
    {% if product.selected_or_first_available_variant.inventory_quantity < 10 %}    <div li-if="product.selected_or_first_available_variant.inventory_quantity < 10" class="products_label is-low-stock">
    <div>Low stock</div>    </div>
{% endif %}    </div>
{% endif %}</div></div></div>
                            
                        </li>
{% endfor %}</ul>{% if recommendations.products_count < 1 %}    <div li-if="recommendations.products_count < 1" class="recommendations_empty">
    <div class="margin-bottom margin-small"><div class="flex-v is-align-centered is-gap-small"><div class="icon-embed-xsmall w-embed"><svg xmlns="http://www.w3.org/2000/svg" width="100%" height="100%" viewbox="0 0 18 18" preserveaspectratio="xMidYMid meet" aria-hidden="true" role="img"><title>grid-empty-obj-bottom-left</title><g fill="none" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" stroke="currentColor"><rect x="10.75" y="2.25" width="5" height="5" rx="1" ry="1" transform="translate(26.5 9.5) rotate(-180)"></rect><rect x="10.75" y="10.75" width="5" height="5" rx="1" ry="1" transform="translate(26.5 26.5) rotate(-180)"></rect><rect x="2.25" y="2.25" width="5" height="5" rx="1" ry="1" transform="translate(9.5 9.5) rotate(-180)"></rect><path d="M7.25,12v-.25c0-.552-.448-1-1-1h-.25"></path><path d="M6,15.75h.25c.552,0,1-.448,1-1v-.25"></path><path d="M2.25,14.5v.25c0,.552,.448,1,1,1h.25"></path><path d="M3.5,10.75h-.25c-.552,0-1,.448-1,1v.25"></path></g></svg></div><div class="text-size-medium text-font-headings text-style-allcaps text-weight-semibold">No similar products found.</div></div></div><a href="/search" class="button is-underscored-link is-alternate w-inline-block"><div>Browse all products</div></a>    </div>
{% endif %}</div></div></div></div><div class="hide w-embed"></div>
